<!DOCTYPE html>
<html>
  <head>
    <title>Table Cube</title>
    <style>
      body {
        /* display: flex; */
        /* justify-content: center; */
        /* align-items: center; */
        /* height: 100vh; */
        /* margin: 0; */
        margin-top: 60px;
      }

      table {
        width: 60%;
        border-collapse: collapse;
        margin: 20px auto;
      }

      h1 {
        margin-left: 300px;
      }

      th,
      td {
        border: 1px solid black;
        padding: 8px;
        text-align: center;
      }

      th {
        background-color: lightgray;
      }
    </style>
  </head>
  <body>
    <h1>Cube Admin</h1>
    <table>
      <tr>
        <th>Date</th>
        <th>Shop Name</th>
        <th>OrderID</th>
        <th>Order Status</th>
        <th>Product Name</th>
        <th>Price</th>
        <th>Quantity</th>
        <th>Total Price</th>
      </tr>
      <% for (let i = 0; i < data.length; i++) { %>
      <tr>
        <td><%= data[i].date %></td>
        <td><%= data[i].sellerName %></td>
        <td><%= data[i].orderId %></td>
        <td><%= data[i].status %></td>
        <td><%= data[i].productName %></td>
        <td><%= data[i].price %></td>
        <td><%= data[i].quantity %></td>
        <td><%= data[i].quantity *  data[i].price %></td>
      </tr>
      <% } %>
    </table>
    <script src="../backend/server.js"></script>
  </body>
</html>
